---
interface Props {
	title: string;
}

const { title } = Astro.props;
---


<head>
    <meta charset="UTF-8" />
    <meta name="description" content="Astro description" />
    <meta name="viewport" content="width=device-width" />
    <link rel="icon" href="/favicon.ico" />
    <meta name="generator" content={Astro.generator} />

    <meta property="og:title" content="Ling Myat Aung" />
    <meta property="og:description" content="Experienced full-stack developer from Myanmar with 2 years of job expertise in Php, Laravel and Vue.js. Ready to create innovative web solutions!" />
    <meta name="description" content="Experienced full-stack developer from Myanmar with 2 years of job expertise in Php, Laravel and Vue.js. Ready to create innovative web solutions!" />

    <meta property="og:type" content="website" />
    <meta property="og:site_name" content="Ling Myat Aung" /> 
    <meta property="og:url" content="https://lingmyataung.vercel.app/">
    <meta property="og:image" content="https://i.ibb.co/wZHxY7p/lingmyataung8.jpg" /> 

    <meta name="twitter:card" content="summary_large_image">
    <meta name="twitter:site" content="Ling Myat Aung" />  
    <meta name="twitter:title" content="Ling Myat Aung" />  
    <meta name="twitter:description" content="TExperienced full-stack developer from Myanmar with 2 years of job expertise in Php, Laravel and Vue.js. Ready to create innovative web solutions!" />  
    <meta name="twitter:image" content="https://i.ibb.co/wZHxY7p/lingmyataung8.jpg" />
    
    <title>{title}</title>
    
</head>